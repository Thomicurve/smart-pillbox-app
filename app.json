{
  "expo": {
    "name": "Smart Pillbox",
    "slug": "smart-pillbox-app",
    "version": "5.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#072F4E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/iconIOS.png",
      "bundleIdentifier": "smartpillbox.pillboxapp"
    },
    "android": {
      "package": "smartpillbox.pillboxapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#072F4E"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#072F4E",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
